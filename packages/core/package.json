{
  "name": "@good-indexer/core",
  "version": "1.0.0",
  "description": "Core types and runtime for good-indexer",
  "keywords": ["indexer", "blockchain", "ethereum", "evm", "types"],
  "type": "module",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "publishConfig": {
    "access": "public"
  },
  "scripts": {
    "build": "tsup src/index.ts --format esm --dts",
    "dev": "tsx watch src/index.ts",
    "lint": "eslint 'src/**/*.{ts,tsx}'",
    "typecheck": "tsc -p tsconfig.json --noEmit",
    "start": "node dist/index.js"
  },
  "dependencies": {
    "@mikro-orm/core": "^6.2.9",
    "@mikro-orm/migrations": "^6.2.9",
    "@mikro-orm/postgresql": "^6.2.9"
  },
  "devDependencies": {
    "@types/node": "^20.14.12",
    "tsup": "^8.3.0",
    "tsx": "^4.19.0",
    "typescript": "^5.6.2"
  }
}
