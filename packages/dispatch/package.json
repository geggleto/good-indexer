{
  "name": "@good-indexer/dispatch",
  "version": "1.0.0",
  "description": "Event dispatching system with inbox pattern for exactly-once processing",
  "keywords": ["indexer", "blockchain", "ethereum", "evm", "dispatch", "events", "inbox"],
  "type": "module",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "publishConfig": {
    "access": "public"
  },
  "scripts": {
    "build": "tsup src/index.ts --format esm --dts",
    "dev": "tsx watch src/index.ts",
    "lint": "eslint 'src/**/*.{ts,tsx}'",
    "typecheck": "tsc -p tsconfig.json --noEmit"
  },
  "dependencies": {
    "@good-indexer/adapters-evm": "workspace:*",
    "@good-indexer/storage-postgres": "workspace:*",
    "@good-indexer/metrics": "workspace:*",
    "@manypkg/find-root": "^2.2.3",
    "@mikro-orm/core": "^6.5.2",
    "@mikro-orm/migrations": "^6.2.9",
    "@mikro-orm/postgresql": "^6.5.2"
  },
  "devDependencies": {
    "@types/node": "^20.14.12",
    "eslint": "^9.10.0",
    "tsup": "^8.3.0",
    "tsx": "^4.19.0",
    "typescript": "^5.6.2"
  }
}
