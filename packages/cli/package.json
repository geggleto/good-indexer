{
  "name": "@good-indexer/cli",
  "version": "1.0.0",
  "description": "Command line interface for good-indexer",
  "keywords": ["indexer", "blockchain", "ethereum", "evm", "cli", "command-line"],
  "type": "module",
  "bin": {
    "gx": "dist/index.js"
  },
  "publishConfig": {
    "access": "public"
  },
  "scripts": {
    "build": "tsup src/index.ts --format esm",
    "dev": "tsx watch src/index.ts",
    "lint": "eslint 'src/**/*.{ts,tsx}'",
    "typecheck": "tsc -p tsconfig.json --noEmit"
  },
  "dependencies": {
    "@good-indexer/adapters-evm": "workspace:*",
    "@good-indexer/dispatch": "workspace:*",
    "@good-indexer/examples-erc20-transfers": "workspace:*",
    "@good-indexer/executor-evm": "workspace:*",
    "@good-indexer/ingest": "workspace:*",
    "@good-indexer/storage-postgres": "workspace:*",
    "@types/commander": "^2.12.5",
    "@manypkg/find-root": "^2.2.3",
    "@mikro-orm/core": "^6.5.2",
    "@mikro-orm/migrations": "^6.2.9",
    "@mikro-orm/postgresql": "^6.5.2",
    "commander": "^12.1.0",
    "dotenv": "^16.4.5"
  },
  "devDependencies": {
    "@types/node": "^20.14.12",
    "tsup": "^8.3.0",
    "tsx": "^4.19.0",
    "typescript": "^5.6.2"
  }
}
