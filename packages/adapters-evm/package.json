{
  "name": "@good-indexer/adapters-evm",
  "version": "1.0.0",
  "description": "EVM RPC adapters with circuit breakers and rate limiting",
  "keywords": ["indexer", "blockchain", "ethereum", "evm", "rpc", "adapters"],
  "type": "module",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "publishConfig": {
    "access": "public"
  },
  "scripts": {
    "build": "tsup src/index.ts --format esm --dts",
    "dev": "tsx watch src/index.ts",
    "lint": "eslint 'src/**/*.{ts,tsx}'",
    "typecheck": "tsc -p tsconfig.json --noEmit"
  },
  "dependencies": {
    "viem": "^2.21.5"
  },
  "devDependencies": {
    "@types/node": "^20.14.12",
    "eslint": "^9.10.0",
    "tsup": "^8.3.0",
    "tsx": "^4.19.0",
    "typescript": "^5.6.2"
  }
}

